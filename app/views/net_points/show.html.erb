<%- model_class = NetPoint -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:latitude) %>:</strong></dt>
  <dd><%= @net_point.latitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:longitude) %>:</strong></dt>
  <dd><%= @net_point.longitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dbm) %>:</strong></dt>
  <dd><%= @net_point.dbm %></dd>
  <dt><strong><%= model_class.human_attribute_name(:asu) %>:</strong></dt>
  <dd><%= @net_point.asu %></dd>
  <dt><strong><%= model_class.human_attribute_name(:mcc) %>:</strong></dt>
  <dd><%= @net_point.mcc %></dd>
  <dt><strong><%= model_class.human_attribute_name(:operator_id) %>:</strong></dt>
  <dd><%= @net_point.operator_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:mnc) %>:</strong></dt>
  <dd><%= @net_point.mnc %></dd>
  <dt><strong><%= model_class.human_attribute_name(:net_type) %>:</strong></dt>
  <dd><%= @net_point.net_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @net_point.user_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              net_points_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_net_point_path(@net_point), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              net_point_path(@net_point),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
